#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/markdown-it@12.3.0/node_modules/markdown-it/bin/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/markdown-it@12.3.0/node_modules/markdown-it/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/markdown-it@12.3.0/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules:/mnt/c/Users/Allen/ui demo/node_modules:/mnt/c/Users/Allen/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/node_modules/markdown-it/bin/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/node_modules/markdown-it/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/markdown-it@12.3.0/node_modules/markdown-it/bin/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/markdown-it@12.3.0/node_modules/markdown-it/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/markdown-it@12.3.0/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules:/mnt/c/Users/Allen/ui demo/node_modules:/mnt/c/Users/Allen/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/node_modules/markdown-it/bin/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/node_modules/markdown-it/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../markdown-it/bin/markdown-it.js" "$@"
else
  exec node  "$basedir/../markdown-it/bin/markdown-it.js" "$@"
fi
