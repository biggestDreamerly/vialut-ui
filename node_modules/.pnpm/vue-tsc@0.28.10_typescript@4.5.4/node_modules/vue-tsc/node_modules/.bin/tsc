#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/typescript@4.5.4/node_modules/typescript/bin/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/typescript@4.5.4/node_modules/typescript/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/typescript@4.5.4/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules:/mnt/c/Users/Allen/ui demo/node_modules:/mnt/c/Users/Allen/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/vue-tsc@0.28.10_typescript@4.5.4/node_modules/vue-tsc/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/vue-tsc@0.28.10_typescript@4.5.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/typescript@4.5.4/node_modules/typescript/bin/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/typescript@4.5.4/node_modules/typescript/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/typescript@4.5.4/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules:/mnt/c/Users/Allen/ui demo/node_modules:/mnt/c/Users/Allen/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/vue-tsc@0.28.10_typescript@4.5.4/node_modules/vue-tsc/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/vue-tsc@0.28.10_typescript@4.5.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.5.4/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.5.4/node_modules/typescript/bin/tsc" "$@"
fi
