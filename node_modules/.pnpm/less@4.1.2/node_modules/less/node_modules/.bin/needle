#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/needle@2.9.1/node_modules/needle/bin/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/needle@2.9.1/node_modules/needle/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/needle@2.9.1/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules:/mnt/c/Users/Allen/ui demo/node_modules:/mnt/c/Users/Allen/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/less@4.1.2/node_modules/less/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/less@4.1.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/needle@2.9.1/node_modules/needle/bin/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/needle@2.9.1/node_modules/needle/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/needle@2.9.1/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules:/mnt/c/Users/Allen/ui demo/node_modules:/mnt/c/Users/Allen/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/less@4.1.2/node_modules/less/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/less@4.1.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../needle@2.9.1/node_modules/needle/bin/needle" "$@"
else
  exec node  "$basedir/../../../../../needle@2.9.1/node_modules/needle/bin/needle" "$@"
fi
