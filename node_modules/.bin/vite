#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/vite@2.7.6_less@4.1.2/node_modules/vite/bin/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/vite@2.7.6_less@4.1.2/node_modules/vite/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/vite@2.7.6_less@4.1.2/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules:/mnt/c/Users/Allen/ui demo/node_modules:/mnt/c/Users/Allen/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/vite/bin/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/vite/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/vite@2.7.6_less@4.1.2/node_modules/vite/bin/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/vite@2.7.6_less@4.1.2/node_modules/vite/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/vite@2.7.6_less@4.1.2/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules:/mnt/c/Users/Allen/ui demo/node_modules:/mnt/c/Users/Allen/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/vite/bin/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/vite/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
